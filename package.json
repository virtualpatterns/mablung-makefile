{
  "author": "virtualpatterns.com <code@virtualpatterns.com> (http://www.virtualpatterns.com)",
  "ava": {
    "failFast": true,
    "files": [
      "release/test/**/*.test.*"
    ],
    "nodeArguments": [
      "--no-warnings",
      "--unhandled-rejections=strict"
    ],
    "require": [],
    "verbose": true
  },
  "c8": {
    "all": true,
    "exclude": [
      "release/test",
      "source/test"
    ],
    "reporter": [
      "html"
    ]
  },
  "description": "TBD",
  "exports": {
    ".": "./release/index.js",
    "./package.json": "./package.json"
  },
  "license": "GPL-3.0+",
  "name": "@virtualpatterns/mablung-makefile",
  "repository": {
    "type": "git",
    "url": "https://github.com/virtualpatterns/mablung-makefile"
  },
  "scripts": {
    "test:10": "make test",
    "test": "run-s --silent \"test:10 argument='{@}'\" --",
    "node:10": "make node",
    "node": "run-s --silent \"node:10 argument='{@}'\" --"
  },
  "type": "module",
  "version": "0.0.1-4",
  "dependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.3",
    "ava": "^3.15.0",
    "c8": "^7.7.2",
    "eslint": "^7.27.0",
    "eslint-plugin-babel": "^5.3.1",
    "npm-check-updates": "^11.5.13",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.3"
  },
  "devDependencies": {
    "@pwn/is": "^1.0.2",
    "depcheck": "^1.4.1",
    "fs-extra": "^10.0.0",
    "json5": "^2.2.0"
  }
}

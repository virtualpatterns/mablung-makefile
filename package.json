{
  "author": "virtualpatterns.com <code@virtualpatterns.com> (http://www.virtualpatterns.com)",
  "ava": {
    "failFast": true,
    "files": [
      "release/test/**/*.test.*"
    ],
    "nodeArguments": [
      "--no-warnings",
      "--unhandled-rejections=strict"
    ],
    "require": [],
    "verbose": true
  },
  "babel": {},
  "bin": {
    "mablung-makefile": "./release/command/mablung-makefile.js"
  },
  "c8": {
    "all": true,
    "exclude": [
      "release/sandbox"
    ],
    "reporter": [
      "html"
    ],
    "src": "release"
  },
  "description": "TBD",
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "requireConfigFile": false
    },
    "plugins": [
      "eslint-plugin-babel"
    ],
    "rules": {
      "arrow-parens": "error",
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-debugger": "warn",
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "no-console": "warn",
      "no-unused-vars": "warn",
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "never"
      ]
    }
  },
  "exports": {
    "./babel": "./release/library/babel.cjs",
    "./eslint": "./release/library/eslint.cjs",
    "./package.json": "./package.json"
  },
  "files": [
    "/include",
    "/release/command",
    "/release/library",
    "makefile"
  ],
  "license": "GPL-3.0+",
  "name": "@virtualpatterns/mablung-makefile",
  "repository": {
    "type": "git",
    "url": "https://github.com/virtualpatterns/mablung-makefile"
  },
  "type": "module",
  "version": "0.0.1-89",
  "dependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/eslint-parser": "^7.14.7",
    "ava": "^3.15.0",
    "c8": "^7.7.3",
    "commander": "^8.0.0",
    "eslint": "^7.31.0",
    "eslint-plugin-babel": "^5.3.1",
    "fs-extra": "^10.0.0",
    "json5": "^2.2.0",
    "npm-check-updates": "^11.8.3",
    "shx": "^0.3.3"
  },
  "devDependencies": {
    "@pwn/is": "^1.0.2",
    "@virtualpatterns/mablung-worker": "0.0.2-40",
    "depcheck": "^1.4.2"
  }
}
